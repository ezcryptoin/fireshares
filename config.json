{
  "api": {
    "baseUrl": "https://api.fairshares.io",
    "appUrl": "https://app.fairshares.io",
    "endpoints": {
      "userInfo": "/user/user_info",
      "taskList": "/task/task-list",
      "taskProgress": "/task/task-progress",
      "taskInfo": "/task/task-info",
      "completeTask": "/task/complete-task",
      "inviteList": "/user/invite_list",
      "evmConnect": "/user_public/evm_connect",
      "joinWaitlist": "/user_public/join_waitlist"
    }
  },
  "referral": {
    "enabled": true,
    "accessCode": "lmir452w",
    "maxReferrals": 3,
    "delayBetweenReferralsMs": {
      "min": 4000,
      "max": 7000
    }
  },
  "scheduler": {
    "checkIntervalMinutes": 60,
    "dailyResetHourUTC": 0,
    "maxActionsPerSession": 15,
    "sessionCooldownMinutes": 30
  },
  "delays": {
    "betweenAccountsMs": 5000,
    "betweenTasksMs": 3000,
    "humanThinkingMinMs": 500,
    "humanThinkingMaxMs": 2000,
    "pageWarmupMs": 2000,
    "variationPercent": 30
  },
  "retry": {
    "maxRetries": 3,
    "baseDelayMs": 1000,
    "maxDelayMs": 30000
  },
  "dummyPages": [
    "/dashboard",
    "/leaderboard",
    "/profile"
  ],
  "security": {
    "neverSavePrivateKey": true,
    "saveOnlyPublicAddress": true,
    "useDeterministicFingerprint": true
  }
}
